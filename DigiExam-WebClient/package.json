{
  "name": "digiexamclient",
  "version": "1.3.1",
  "repository": "https://github.com/DigiExam/DigiExam-Webclient/",
  "scripts": {
    "postinstall": "./node_modules/.bin/bower install",
    "build-modules-x64": "./platforms/electron/node/build.sh x64",
    "build-modules-x86": "./platforms/electron/node/build.sh ia32",
    "package:osx": "electron-packager ./bin/ 'DigiExam Client' --platform=darwin --arch=all --version=0.30.4 --overwrite --icon ./src/appIcon.icns",
    "package:win32": "electron-packager ./bin/ 'DigiExam Client' --platform=win32 --arch=ia32 --version=0.30.4 --overwrite --icon ./src/appIcon.icns",
    "package:win64": "electron-packager ./bin/ 'DigiExam Client' --platform=win32 --arch=x64 --version=0.30.4 --overwrite --icon ./src/appIcon.icns",
    "pack:osx": "npm run build:osx && electron-builder \"DigiExam Client darwin-x64/DigiExam Client.app\" --platform=osx --out=\"dist/osx\" --config=./packager.json",
    "win:postBuild": "wine start ResourceHacker.exe -modify './DigiExam Client-win32-ia32/DigiExam Client.exe', './DigiExam Client-win32-ia32/DigiExam Client.exe', './src/AppIcon.icns', ICONGROUP, MAINICON, 0"
   },
  "devDependencies": {
    "bower": "~1.4.1",
    "electron-packager": "^5.0.2",
    "gulp": "~3.9.0",
    "gulp-autoprefixer": "~2.3.1",
    "gulp-bytediff": "~0.2.1",
    "gulp-coffee": "~2.3.1",
    "gulp-coffeelint": "~0.5.0",
    "gulp-concat": "~2.5.2",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "~0.14.0",
    "gulp-file-include": "~0.9.0",
    "gulp-filter": "~2.0.2",
    "gulp-help": "~1.6.0",
    "gulp-htmlhint": "~0.1.1",
    "gulp-if": "~1.2.4",
    "gulp-load-plugins": "~0.5.3",
    "gulp-minify-css": "~1.0.0",
    "gulp-rename": "~1.2.0",
    "gulp-replace": "~0.5.3",
    "gulp-rev-hash": "EvilOatmeal/gulp-rev-hash#preserve-original-tag",
    "gulp-rimraf": "~0.1.0",
    "gulp-ruby-sass": "~1.0.1",
    "gulp-sourcemaps": "~1.1.1",
    "gulp-uglify": "~0.3.1",
    "gulp-util": "~3.0.0",
    "gulp-zip": "~3.0.2",
    "html-minifier": "~0.7.0",
    "karma": "~0.12.23",
    "karma-chrome-launcher": "~0.1.4",
    "karma-coffee-preprocessor": "~0.2.1",
    "karma-coverage": "~0.2.7",
    "karma-firefox-launcher": "~0.1.3",
    "karma-jasmine": "~0.3.5",
    "karma-ng-html2js-preprocessor": "~0.1.0",
    "karma-phantomjs-launcher": "~0.2.0",
    "run-sequence": "~1.1.0",
    "sassdown": "~0.2.6",
    "scribe-angular": "DigiExam/scribe-angularjs#1.0.0",
    "scribe-plugin-merge-adjacent-lists": "DigiExam/scribe-plugin-merge-adjacent-lists#1.0.0",
    "scribe-plugin-word-count": "DigiExam/scribe-plugin-word-count#1.0.0",
    "stream-combiner2": "~1.0.2",
    "underscore": "~1.8.3",
    "web-ui": "DigiExam/web-ui"
  },
  "dependencies": {
    "electron-builder": "^2.0.0",
    "electron-prebuilt": "^0.31.2"
  }
}
